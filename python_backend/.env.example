# Required for JWT signing
JWT_SECRET=change_me_long_random_string

# Username and password hash for login.
# Generate a password hash with werkzeug.security.generate_password_hash("your_password")
AUTH_USERNAME=admin
AUTH_PASSWORD_HASH=pbkdf2:sha256:150000$...  # put the generated hash here

# Third-party API key used by resume generation logic
RESUME_API_KEY=put_your_real_api_key_here
